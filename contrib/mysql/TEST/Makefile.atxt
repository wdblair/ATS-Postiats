%{

fun comment
  (x: string): atext = atext_nil ()
// end of [comment]

fun make_entry
  (nm: string): atext = let
//
val ent = sprintf ("\
all:: %s\n\
%s: %s.exe\n\
%s.exe: %s_dats.c ; \\\n\
  $(GCC) $(CCFLAGS) \\\n\
    -I$(PATSHOMEQ) \\\n\
    -I$(PATSHOMEQ)/ccomp/runtime \\\n\
    -I$(PATSHOMEQ)/contrib \\\n\
    $(MYSQLCFLAGS) \\\n\
    -o $@ $< \\\n\
    $(MYSQLLIBS) \n\
%s_dats.c: %s.dats ; \\\n\
  $(PATSOPT) --output %s_dats.c --dynamic %s.dats\n\
clean:: ; $(RMF) %s_dats.o\n\
clean:: ; $(RMF) %s_dats.c\n\
cleanall:: ; $(RMF) %s.exe\n\
", @(nm, nm, nm, nm, nm, nm, nm, nm, nm, nm, nm, nm)
) // end of [val]
//
val ent = string_of_strptr (ent)
in
  atext_strcst (ent)
end // end of [make_entry]

%}\
